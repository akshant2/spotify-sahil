{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "yarn tailwind:watch & react-scripts start",
    "build": "yarn tailwind && export PUBLIC_URL=/resources/react && react-scripts build && gulp",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "tailwind": "postcss tailwind/tailwind.css -o src/styles/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i tailwind/tailwind.css -o src/styles/tailwind.css --watch",
    "lint-staged": "lint-staged -c .lintstagedrc.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "through2": "^4.0.2"
  }
}
